{
  "name": "nuxt-attribute-content",
  "type": "module",
  "version": "0.1.8",
  "packageManager": "pnpm@10.15.1",
  "main": "./nuxt.config.ts",
  "scripts": {
    "dev": "nuxi dev .playground",
    "dev:prepare": "nuxt prepare .playground",
    "build": "nuxt build .playground",
    "generate": "nuxt generate .playground",
    "preview": "nuxt preview .playground",
    "lint": "npx oxlint --type-aware && npx eslint",
    "release": "npm run lint && npm run test && changelogen --release && npm publish && git push --follow-tags",
    "test": "vitest run",
    "test:watch": "vitest watch --ui --coverage.enabled=true --coverage.reporter=html",
    "test:types": "vue-tsc --noEmit && cd playground && vue-tsc --noEmit",
    "prepare": "husky"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@nuxt/eslint": "latest",
    "@nuxt/test-utils": "^3.19.2",
    "@nuxtjs/seo": "3.2.2",
    "@vitest/coverage-v8": "3.2.4",
    "@vitest/ui": "3.2.4",
    "@vue/test-utils": "^2.4.6",
    "changelogen": "^0.6.2",
    "eslint": "^9.37.0",
    "eslint-plugin-oxlint": "^1.25.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.6",
    "nuxt": "^4.1.3",
    "oxlint": "^1.25.0",
    "oxlint-tsgolint": "^0.5.0",
    "playwright": "^1.56.0",
    "playwright-core": "^1.56.0",
    "postcss-custom-media": "^11.0.6",
    "postcss-nested": "^7.0.2",
    "svgo": "^4.0.0",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4",
    "vue": "latest",
    "vue-tsc": "^3.1.1",
    "zod": "^4.1.12"
  },
  "dependencies": {
    "@napi-rs/canvas": "^0.1.80",
    "@nuxt/content": "^3.7.1",
    "@nuxt/image": "^1.11.0",
    "@nuxtjs/leaflet": "^1",
    "@unlazy/nuxt": "^1.0.0",
    "better-sqlite3": "^12.4.1",
    "happy-dom": "^18.0.1",
    "image-meta": "^0.2.2",
    "markdown-it": "^14.1.0",
    "nuxt-ipx-cache": "1.0.6",
    "nuxt-svg-icon-sprite": "2.0.2",
    "nuxt3-interpolation": "^1.0.16",
    "thumbhash": "^0.1.1"
  },
  "peerDependencies": {
    "@nuxtjs/leaflet": "^1"
  },
  "peerDependenciesMeta": {
    "@nuxtjs/leaflet": {
      "optional": true
    }
  },
  "lint-staged": {
    "**/*.{js,mjs,cjs,jsx,ts,mts,cts,tsx,vue,astro,svelte}": "oxlint --type-aware"
  }
