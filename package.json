{
  "name": "nuxt-attribute-content",
  "type": "module",
  "version": "0.1.1",
  "packageManager": "pnpm@10.15.1",
  "main": "./nuxt.config.ts",
  "scripts": {
    "dev": "nuxi dev .playground",
    "dev:prepare": "nuxt prepare .playground",
    "build": "nuxt build .playground",
    "generate": "nuxt generate .playground",
    "preview": "nuxt preview .playground",
    "lint": "eslint .",
    "release": "npm run lint && npm run test && changelogen --release && npm publish && git push --follow-tags",
    "test": "vitest run",
    "test:watch": "vitest watch --ui --coverage.enabled=true --coverage.reporter=html",
    "test:types": "vue-tsc --noEmit && cd playground && vue-tsc --noEmit"
  },
  "devDependencies": {
    "@nuxt/eslint": "latest",
    "@nuxt/test-utils": "^3.19.2",
    "@vitest/coverage-v8": "3.2.4",
    "@vitest/ui": "3.2.4",
    "@vue/test-utils": "^2.4.6",
    "changelogen": "^0.6.2",
    "eslint": "^9.34.0",
    "nuxt": "^4.1.1",
    "nuxt-ipx-cache": "1.0.6",
    "playwright": "^1.55.0",
    "playwright-core": "^1.55.0",
    "postcss-custom-media": "^11.0.6",
    "postcss-nested": "^7.0.2",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4",
    "vue": "latest",
    "vue-tsc": "^3.0.6"
  },
  "dependencies": {
    "@nuxt/content": "^3.6.3",
    "@nuxt/image": "^1.11.0",
    "better-sqlite3": "^12.2.0",
    "happy-dom": "^18.0.1",
    "markdown-it": "^14.1.0",
    "nuxt-lazyimages": "^0.2.1",
    "nuxt-svg-icon-sprite": "2.0.2"
  },
  "peerDependencies": {
    "@nuxtjs/leaflet": "^1"
  },
  "peerDependenciesMeta": {
    "@nuxtjs/leaflet": {
      "optional": true
    }
  }
}
